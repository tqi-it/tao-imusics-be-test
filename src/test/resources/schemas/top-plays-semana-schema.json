{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": [
    "mensagem",
    "codigo",
    "dados",
    "qde_registros",
    "pagina",
    "qde_por_pagina",
    "somaTotalRegistros"
  ],
  "properties": {
    "mensagem": {
      "type": "string",
      "description": "Mensagem de retorno da requisição."
    },
    "codigo": {
      "type": "integer",
      "description": "Código numérico do retorno da API."
    },
    "dados": {
      "type": "array",
      "description": "Lista contendo detalhes das execuções das faixas.",
      "items": {
        "type": "object",
        "required": [
          "artistas",
          "tituloMusica",
          "qdePlays",
          "percentual"
        ],
        "properties": {
          "artistas": {
            "type": "string",
            "minLength": 1
          },
          "tituloMusica": {
            "type": "string",
            "minLength": 1
          },
          "qdePlays": {
            "type": "integer",
            "minimum": 0
          },
          "percentual": {
            "type": "number"
          }
        },
        "additionalProperties": false
      }
    },
    "qde_registros": {
      "type": "integer"
    },
    "pagina": {
      "type": "integer"
    },
    "qde_por_pagina": {
      "type": "integer"
    },
    "somaTotalRegistros": {
      "type": "integer"
    }
  },
  "additionalProperties": false
}